<h1>Top画面</h1>

<form action="top/get" method="post" name="pre_data">
    <select name="prefecture">
        <% @select_box_pref_data.each do |pref_data| %>
            <option value="<%= pref_data['prefCode'] %>"><%= pref_data['prefName'] %></option>
        <% end %>
    </select>

    <input type="submit" value="検索">
</form>